{
  "name": "sajari-react",
  "description": "React SDK for the Sajari API",
  "version": "1.5.1",
  "author": [
    {
      "name": "Trent Billington",
      "email": "tbillington@sajari.com",
      "url": "https://www.sajari.com"
    },
    {
      "name": "David Howden",
      "email": "dhowden@sajari.com",
      "url": "https://www.sajari.com"
    }
  ],
  "homepage": "https://github.com/sajari/sajari-sdk-react",
  "bugs": "https://github.com/sajari/sajari-sdk-react/issues",
  "contributors": [
    {
      "name": "Sajari Support <support@sajari.com>",
      "url": "https://www.sajari.com"
    }
  ],
  "dependencies": {
    "prop-types": "^15.5.10",
    "sajari": "^0.20.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.42",
    "@babel/core": "^7.0.0-beta.42",
    "@babel/node": "^7.0.0-beta.42",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.42",
    "@babel/polyfill": "^7.0.0-beta.42",
    "@babel/preset-env": "^7.0.0-beta.42",
    "@babel/preset-react": "^7.0.0-beta.42",
    "@babel/preset-stage-3": "^7.0.0-beta.42",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "7",
    "babel-jest": "^23.0.0-alpha.0",
    "eslint": "^4.3.0",
    "eslint-plugin-class-property": "^1.0.6",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-react": "^7.1.0",
    "fs-extra": "^0.30.0",
    "jest": "^22.4.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "regenerator-runtime": "^0.11.1",
    "rimraf": "^2.5.3"
  },
  "keywords": [
    "autocomplete",
    "match",
    "realtime",
    "recommend",
    "sajari",
    "search",
    "search api",
    "site search",
    "website search"
  ],
  "license": "MIT",
  "main": "./src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/sajari/sajari-sdk-react"
  },
  "scripts": {
    "build":
      "npm run build:babel && npm run build:babel:es && npm run build:copy-files",
    "build:babel":
      "BABEL_ENV=std babel --extends .babelrc --out-dir ./build --ignore '*.test.js' --copy-files --delete-dir-on-start ./src",
    "build:babel:es":
      "babel --out-dir ./build/es --ignore '*.test.js' --copy-files --delete-dir-on-start ./src",
    "build:copy-files":
      "babel-node --presets=@babel/preset-env ./scripts/copy-files.js",
    "lint": "eslint src/*",
    "clean:build": "rimraf build/*",
    "build:watch":
      "npm run clean:build && npm run build:copy-files && npm run build:babel -- --watch",
    "prebuild": "eslint src/* && jest && npm run clean:build",
    "test": "jest"
  }
}
