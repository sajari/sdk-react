{
  "name": "sajari-sdk-react",
  "version": "1.0.0",
  "scripts": {
    "build": "npm run build:babel && npm run build:copy-files",
    "build:babel": "babel ./src/ --out-dir ./build",
    "build:copy-files": "babel-node ./scripts/copy-files.js",
    "clean:build": "rimraf build",
    "prebuild": "npm run clean:build",
    "dev": "npm run prebuild && npm run build:copy-files && babel -w ./src/ --out-dir ./build"
  },
  "author": {
    "name": "Trent Billington",
    "email": "tbillington@sajari.com",
    "url": "https://www.sajari.com"
  },
  "license": "MIT",
  "keywords": [
    "sajari",
    "search",
    "search api",
    "match",
    "recommend",
    "realtime",
    "autocomplete"
  ],
  "description": "React SDK for accessing the Sajari search and recommendations API",
  "repository": {
    "type": "git",
    "url": "https://github.com/sajari/sajari-sdk-react"
  },
  "main": "./src/index.js",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "fs-extra": "^0.30.0",
    "immutable": "^3.8.1",
    "mocha": "^2.2.5",
    "rimraf": "^2.5.3",
    "uuid": "^2.0.2",
    "webpack": "^1.9.10",
    "webpack-dev-server": "^1.8.2"
  },
  "dependencies": {
    "eventemitter3": "^0.1.6",
    "flux": "^2.0.1",
    "keymirror": "^0.1.0",
    "loglevel": "^1.4.1",
    "object-assign": "^2.0.0",
    "protobufjs": "^5.0.1",
    "superagent": "^2.0.0",
    "uuid": "^2.0.2"
  },
  "peerDependencies": {
    "react": "^15.0.0",
    "react-dom": "^15.0.0",
    "sajari": "*"
  }
}
