{
  "presets": [
    "@emotion/babel-preset-css-prop",
    [
      "@babel/env",
      {
        "modules": false,
        "targets": {
          "browsers": [">0.25%", "ie > 9", "not op_mini all"]
        }
      }
    ],
    "@babel/react"
  ],
  "plugins": [
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread",
    ["babel-plugin-emotion", { "hoist": true }],
    "babel-plugin-idx"
  ]
}
