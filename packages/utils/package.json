{
  "name": "@sajari/react-sdk-utils",
  "version": "2.0.0",
  "description": "Shared utils for the Sajari React packages.",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "directory": "packages/utils"
  },
  "scripts": {
    "start": "tsdx watch --tsconfig tsconfig.build.json --tsdxconfig --verbose --noClean",
