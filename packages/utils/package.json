    "@tailwindcss/typography": "^0.4.1",
    "emotion-theming": "^10.0.27",
    "tailwindcss": "^2.2.19",
    "tailwindcss-filters": "^3.0.0",
    "tailwindcss-truncate-multiline": "^1.0.3",
    "twin.macro": "^2.8.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "d2b9639c742e1dace15317d57a409777cd736dee",
